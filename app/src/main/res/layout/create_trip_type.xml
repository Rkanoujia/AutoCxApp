<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:background="@color/gray"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include
            layout="@layout/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    <LinearLayout

        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/fram_cn_to_cn"
            android:elevation="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:textStyle="italic|bold"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:elevation="3dp"
                android:fontFamily="@font/open_sans"
                android:gravity="center"
                android:padding="5dp"
                android:text="Inter Provincial"
                android:textColor="@color/textColor"
                android:textSize="14dp" />
            <LinearLayout
                android:id="@+id/cn_to_cn"
                android:background="@android:color/white"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:gravity="end|center_vertical"
                    android:elevation="3dp"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                   <View
                       android:layout_marginRight="5dp"
                       android:background="@mipmap/cn"
                       android:layout_width="55dp"
                       android:layout_height="55dp" />


                </LinearLayout>
                <FrameLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <View
                        android:elevation="2dp"
                        android:layout_gravity="start|center"
                        android:backgroundTint="@color/dot_inactive_screen2"
                        android:background="@drawable/ovel"
                        android:layout_width="8dp"
                        android:layout_height="8dp"/>
                    <View
                        android:background="@android:color/darker_gray"
                        android:layout_gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"/>
                    <View
                        android:elevation="2dp"
                        android:layout_gravity="end|center"
                        android:backgroundTint="@android:color/holo_red_light"
                        android:background="@drawable/ovel"
                        android:layout_width="8dp"
                        android:layout_height="8dp"/>

                </FrameLayout>
                <LinearLayout
                    android:gravity="start|center_vertical"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <View
                        android:layout_marginStart="5dp"
                        android:background="@mipmap/cn"
                        android:layout_width="55dp"
                        android:layout_height="55dp" />
                </LinearLayout>
            </LinearLayout>



        </FrameLayout>
        <FrameLayout
            android:id="@+id/fram_cn_to_us"
            android:elevation="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:fontFamily="@font/open_sans"
                android:padding="5dp"
                android:gravity="center"
                android:elevation="3dp"
                android:textStyle="italic|bold"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="Export"
                android:textColor="@color/textColor"
                android:textSize="14dp" />
            <LinearLayout
                android:id="@+id/cn_to_us"
                android:background="@android:color/white"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:gravity="end|center_vertical"
                    android:elevation="3dp"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <View
                        android:layout_marginRight="5dp"
                        android:background="@mipmap/cn"
                        android:layout_width="55dp"
                        android:layout_height="55dp" />


                </LinearLayout>
                <FrameLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <View
                        android:elevation="2dp"
                        android:layout_gravity="start|center"
                        android:backgroundTint="@color/dot_inactive_screen2"
                        android:background="@drawable/ovel"
                        android:layout_width="8dp"
                        android:layout_height="8dp"/>
                    <View
                        android:background="@android:color/darker_gray"
                        android:layout_gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"/>
                    <View
                        android:elevation="2dp"
                        android:layout_gravity="end|center"
                        android:backgroundTint="@android:color/holo_red_light"
                        android:background="@drawable/ovel"
                        android:layout_width="8dp"
                        android:layout_height="8dp"/>

                </FrameLayout>
                <LinearLayout
                    android:gravity="start|center_vertical"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <View
                        android:layout_marginStart="5dp"
                        android:background="@mipmap/us"
                        android:layout_width="55dp"
                        android:layout_height="55dp" />
                </LinearLayout>
            </LinearLayout>



        </FrameLayout>
        <FrameLayout
            android:id="@+id/fram_us_to_cn"
            android:elevation="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:fontFamily="@font/open_sans"
                android:padding="5dp"
                android:gravity="center"
                android:elevation="3dp"
                android:layout_width="120dp"
                android:textStyle="italic|bold"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="Import"
                android:textColor="@color/textColor"
                android:textSize="14dp" />
            <LinearLayout
                android:id="@+id/us_to_cn"
                android:background="@android:color/white"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:gravity="end|center_vertical"
                    android:elevation="3dp"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <View
                        android:layout_marginRight="5dp"
                        android:background="@mipmap/us"
                        android:layout_width="55dp"
                        android:layout_height="55dp" />


                </LinearLayout>
                <FrameLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <View
                        android:elevation="2dp"
                        android:layout_gravity="start|center"
                        android:backgroundTint="@color/dot_inactive_screen2"
                        android:background="@drawable/ovel"
                        android:layout_width="8dp"
                        android:layout_height="8dp"/>
                    <View
                        android:background="@android:color/darker_gray"
                        android:layout_gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"/>
                    <View
                        android:elevation="2dp"
                        android:layout_gravity="end|center"
                        android:backgroundTint="@android:color/holo_red_light"
                        android:background="@drawable/ovel"
                        android:layout_width="8dp"
                        android:layout_height="8dp"/>

                </FrameLayout>
                <LinearLayout
                    android:gravity="start|center_vertical"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <View
                        android:layout_marginStart="5dp"
                        android:background="@mipmap/cn"
                        android:layout_width="55dp"
                        android:layout_height="55dp" />
                </LinearLayout>
            </LinearLayout>



        </FrameLayout>
        <FrameLayout
            android:id="@+id/fram_us_to_us"
            android:elevation="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:fontFamily="@font/open_sans"
                android:padding="5dp"
                android:id="@+id/type"
                android:gravity="center"
                android:elevation="3dp"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="Inter State"
                android:textStyle="italic|bold"
                android:textColor="@color/textColor"
                android:textSize="14dp" />
            <LinearLayout
                android:id="@+id/us_to_us"
                android:background="@android:color/white"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:gravity="end|center_vertical"
                    android:elevation="3dp"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <View
                        android:layout_marginRight="5dp"
                        android:background="@mipmap/us"
                        android:layout_width="55dp"
                        android:layout_height="55dp" />


                </LinearLayout>
                <FrameLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <View
                        android:elevation="2dp"
                        android:layout_gravity="start|center"
                        android:backgroundTint="@color/dot_inactive_screen2"
                        android:background="@drawable/ovel"
                        android:layout_width="8dp"
                        android:layout_height="8dp"/>
                    <View
                        android:background="@android:color/darker_gray"
                        android:layout_gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"/>
                    <View
                        android:elevation="2dp"
                        android:layout_gravity="end|center"
                        android:backgroundTint="@android:color/holo_red_light"
                        android:background="@drawable/ovel"
                        android:layout_width="8dp"
                        android:layout_height="8dp"/>

                </FrameLayout>
                <LinearLayout
                    android:gravity="start|center_vertical"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <View
                        android:layout_marginStart="5dp"
                        android:background="@mipmap/us"
                        android:layout_width="55dp"
                        android:layout_height="55dp" />
                </LinearLayout>
            </LinearLayout>



        </FrameLayout>

    </LinearLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>